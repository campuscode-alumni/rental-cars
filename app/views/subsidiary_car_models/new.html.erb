

<%= form_for @subsidiary_car_model do |f| %>
  <div class="form-group">
    <%= f.label :car_model_id, 'Selecione o modelo' %>
    <%= f.collection_select :car_model_id, @car_models, :id, :name, {}, class: 'form-control' %>
  </div>

  <div class="form-group">
    <%= f.label :price, 'Preço' %>
    <%= f.text_field :price, class: 'form-control' %>
  </div>
  
  <%= f.submit 'Cadastrar preço', class: 'btn btn-primary' %>
<% end %>
